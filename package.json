{
  "name": "prisma-extension-kysely",
  "version": "4.0.0",
  "author": {
    "name": "Eoin O'Brien",
    "url": "https://eoin.ai",
    "email": "eoin@tracworx.ai"
  },
  "description": "Prisma extension for Kysely",
  "license": "MIT",
  "homepage": "https://github.com/eoin-obrien/prisma-extension-kysely#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/eoin-obrien/prisma-extension-kysely"
  },
  "bugs": {
    "url": "https://github.com/eoin-obrien/prisma-extension-kysely/issues"
  },
  "keywords": [
    "prisma",
    "@prisma/client",
    "extension",
    "kysely",
    "sql",
    "query builder",
    "orm",
    "database",
    "postgresql",
    "mysql",
    "sqlite"
  ],
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "default": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": ["dist", "!**/*.tsbuildinfo"],
  "scripts": {
    "pretest": "prisma db push && prisma generate",
    "test": "vitest run",
    "test:watch": "vitest",
    "pretest:coverage": "prisma db push && prisma generate",
    "test:coverage": "vitest run --coverage",
    "build:clean": "rm -rf ./dist",
    "build:compile": "tsc -b tsconfig.json tsconfig.cjs.json && tsconfig-to-dual-package",
    "build": "npm run build:clean && npm run build:compile",
    "prepack": "npm run build",
    "prepare": "husky || true",
    "commit": "git-cz",
    "check": "biome check .",
    "check:write": "biome check --write ."
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "peerDependencies": {
    "@prisma/client": "^7.0.0",
    "kysely": "^0.27.0 || ^0.28.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.4.4",
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/cz-commitlint": "^20.0.0",
    "@prisma/adapter-better-sqlite3": "^7.4.1",
    "@prisma/client": "^7.0.0",
    "@prisma/extension-read-replicas": "^0.5.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@vitest/coverage-v8": "^3.0.0",
    "commitizen": "^4.3.0",
    "dotenv": "^17.3.1",
    "husky": "^9.0.0",
    "inquirer": "^13.3.0",
    "kysely": "^0.28.0",
    "prisma": "^7.0.0",
    "prisma-kysely": "^3.0.0",
    "semantic-release": "^25.0.3",
    "tsconfig-to-dual-package": "^1.2.0",
    "typescript": "^5.0.0",
    "vitest": "^3.0.0"
  }
}
